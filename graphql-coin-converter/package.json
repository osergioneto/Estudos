{
  "name": "currency_conversion",
  "version": "1.0.0",
  "scripts": {
    "test": "jest --runInBand --forceExit --coverage",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev:start": "npm run codegen && ts-node src/server.ts",
    "build": "npm run codegen && tsc",
    "migrate": "npx sequelize db:migrate",
    "dbcreate": "npx sequelize db:create",
    "start": "npm run migrate && node -r source-map-support/register dist/server.js",
    "check": "tsc --noEmit",
    "check-prettier": "prettier --list-different {src,spec}/*.ts {src,spec}/**/*.ts {src,spec}/**/**/*.ts {src,spec}/**/**/**/*.ts",
    "fix-prettier": "prettier --write {src,spec}/*.ts {src,spec}/**/*.ts {src,spec}/**/**/*.ts {src,spec}/**/**/**/*.ts",
    "lint": "tslint --project ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.6.4",
    "apollo-server-testing": "^2.9.3",
    "bcrypt": "^3.0.6",
    "dataloader": "^1.4.0",
    "dotenv": "^8.0.0",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^5.5.0",
    "sequelize-typescript": "^1.0.0-beta.3",
    "slugify": "^1.3.5",
    "source-map-support": "^0.5.12",
    "tslint": "^5.19.0",
    "typescript": "^3.5.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.5",
    "@graphql-codegen/introspection": "1.3.0",
    "@graphql-codegen/typescript": "1.3.0",
    "@graphql-codegen/typescript-resolvers": "1.3.0",
    "@types/bcrypt": "^3.0.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/uuid": "^7.0.3",
    "jest": "^25.5.4",
    "prettier": "1.18.2",
    "ts-jest": "^25.4.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1"
  }
}
